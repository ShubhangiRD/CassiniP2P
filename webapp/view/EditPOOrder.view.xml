<mvc:View controllerName="com.cassiniProcureToPay.controller.EditPOOrder" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	xmlns:u="sap.ui.unified" xmlns:t="sap.ui.table" xmlns:f="sap.ui.layout.form" displayBlock="true" xmlns:html="http://www.w3.org/1999/xhtml">
	<core:Fragment fragmentName="com.cassiniProcureToPay.view.fragment.Header" type="XML"/>
	<Page title="" class="sapUiTinyMarginBegin sapUiSmallMarginEnd">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Label id="ed" text="Display Purchase Order"/>
				<ToolbarSpacer/>
				<Button id="idEdit" icon="sap-icon://edit" type="Transparent" tooltip="Edit" press="onEditPOOrders"/>
			</Toolbar>
		</customHeader>
		<content>
			<VBox fitContainer="true" class="sapUiResponsiveMargin ">
				<f:Form id="FormLumpSum" editable="false">
				<!--	<f:layout>
						<f:ResponsiveGridLayout labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
							emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false"/>
					</f:layout>-->
						<f:layout>
						<f:ResponsiveGridLayout labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
							emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="4" columnsL="4" columnsM="4" singleContainerFullSize="false"/>
					</f:layout>
					<f:formContainers>
						<f:FormContainer>
							<f:formElements>
								<f:FormElement label="Vendor Number">
									<f:fields>
										<Input id="vnumber" textFormatMode="KeyValue" placeholder="Vendor" showSuggestion="true" showValueHelp="true"
											value="{PurchaseModel>/TempContract/Lifnr}" valueHelpRequest="handleMaterialValueHelp"
											suggestionItems="{ path: 'Lookup>/DisplyaVendorList' }" suggestionItemSelected="suggestionVebor">
											<suggestionItems>
												<core:ListItem key="{Lookup>Lifnr}" text="{Lookup>Name1}" additionalText="{Lookup>Lifnr}"/>
											</suggestionItems>
										</Input>
										<!--	<Input
											id="vnumber"
											textFormatMode="KeyValue"
											placeholder=" Vendor Number"
											value="{PurchaseModel>/TempContract/Lifnr}"
											showSuggestion="true"
											showValueHelp="true"
											valueHelpRequest="handlePOVHelpVendor"
											suggestionItems="{ path: 'Lookup>/POVendorList' }"
											suggestionItemSelected="suggestionItemSelectedVendor">
											<suggestionItems>
												<core:ListItem
												key="{Lookup>Lifnr}" text="{Lookup>Name1}" additionalText="{Lookup>Lifnr}" />
											</suggestionItems>
										</Input>
								-->
									</f:fields>
								</f:FormElement>
								<f:FormElement label="Purchase Org">
									<f:fields>
										<Input id="idPurOrg" textFormatMode="KeyValue" placeholder=" Purchase Org .." showSuggestion="true"
											value="{PurchaseModel>/TempContract/Ekorg}" showValueHelp="true" valueHelpRequest="handlePurchaseOrgVendor"
											suggestionItems="{ path: 'Lookup>/PurchaseOrganization' }" editable="{EditModel>/isEditable}" suggestionItemSelected="onSuggest">
											<suggestionItems>
												<core:ListItem key="{Lookup>Ekorg}" text="{Lookup>Ekotx}" additionalText="{Lookup>Ekorg}"/>
											</suggestionItems>
										</Input>
										<!--<Input type="Text" id="idPurOrg" value="{PurchaseModel>/TempContract/Ekorg}" editable="{EditModel>/isEditable}"></Input>
						-->
									</f:fields>
								</f:FormElement>
							</f:formElements>
						</f:FormContainer>
					</f:formContainers>
					<f:formContainers>
						<f:FormContainer>
							<f:formElements>
								<f:FormElement label="Purchase Doc Numebr">
									<f:fields>
										<Input id="productPO" textFormatMode="KeyValue" placeholder="Purchase Document" showSuggestion="true" showValueHelp="true"
											valueHelpRequest="handlePursOrderValueHelp" suggestionItems="{ path: 'Lookup>/POOrderList'}"
											suggestionItemSelected="suggestionItemSelectedPOrder">
											<suggestionItems>
												<core:ListItem key="{Lookup>Ebeln}" text="{Lookup>Lifnr}" additionalText="{Lookup>Ebeln}"/>
											</suggestionItems>
										</Input>
										<!--	<Input id="idPoOrder" 
									value="{PurchaseModel>/TempContract/Ebeln}" 
									placeholder =" Purchase Order Number ..." 
									showSuggestion="true"
									showValueHelp="true" 
									suggest="onSuggestPON"  
									valueHelpRequest="onValueHelpRequestPOrder"
									suggestionItemSelected="suggestionItemSelectedPOrder"
									suggestionItems="{VHeader>/POHeaderSet}">
								
									<suggestionItems>
										<core:Item key="{VHeader>Eblen}"/>
									</suggestionItems>
								</Input>-->
									</f:fields>
								</f:FormElement>
								<f:FormElement label=" Currency">
									<f:fields>
										<Input type="Text" id="idCountryCode" value="{PurchaseModel>/TempContract/Waers}"></Input>
									</f:fields>
								</f:FormElement>
							</f:formElements>
						</f:FormContainer>
					</f:formContainers>
					<f:formContainers>
						<f:FormContainer>
							<f:formElements>
								<f:FormElement label="Company Code">
									<f:fields>
										<Input id="idCompCode" textFormatMode="KeyValue" placeholder="Company Code ..." showSuggestion="true"
											value="{PurchaseModel>/TempContract/Bukrs}" showValueHelp="true" valueHelpRequest="handleCompanyCodeVendor"
											editable="{EditModel>/isEditable}" suggestionItems="{ path: 'Lookup>/CountryCode' }" suggestionItemSelected="onSuggest">
											<suggestionItems>
												<core:ListItem key="{Lookup>Bukrs}" text="{Lookup>Butxt}" additionalText="{Lookup>Bukrs}"/>
											</suggestionItems>
										</Input>
										<!--	<Input type="Text" id="idCompCode" value="{PurchaseModel>/TempContract/Bukrs}" ></Input>
						-->
									</f:fields>
								</f:FormElement>
								<f:FormElement label="Purchase Group">
									<f:fields>
										<Input id="idPurGrg" textFormatMode="KeyValue" placeholder=" Purchase Grp ..." showSuggestion="true"
											value="{PurchaseModel>/TempContract/Ekgrp}" editable="{EditModel>/isEditable}" showValueHelp="true"
											valueHelpRequest="handlePurchaseGroupVendor" suggestionItems="{ path: 'Lookup>/PurchaseGroupList' }" suggestionItemSelected="onSuggest">
											<suggestionItems>
												<core:ListItem key="{Lookup>Ekgrp}" text="{Lookup>Eknam}" additionalText="{Lookup>Ekgrp}"/>
											</suggestionItems>
										</Input>
										<!--		<Input type="Text" id="idPurGrg" value="{PurchaseModel>/TempContract/Ekgrp}" ></Input>
					-->
									</f:fields>
								</f:FormElement>
							</f:formElements>
						</f:FormContainer>
					</f:formContainers>
				</f:Form>
				<t:Table id="idPOItemsTab"  rows="{ path: 'PurchaseModel>/TempContract/POItem'}"
					visibleRowCount="5" selectionMode="MultiToggle">
					<t:extension>
						<OverflowToolbar>
							<Title text="Create Purchase Order"/>
							<ToolbarSpacer/>
							<Button icon="sap-icon://add" type="Transparent" id="idAddbutton" class="remove-attr" tooltip="Add Row" press="onAddNewConditionItem"/>
							<Button icon="sap-icon://delete" enabled="{=${PurchaseModel>/TempContract/RebateConditionItems}.length > 0}" type="Transparent"
								tooltip="Delete Row" press="onDeleteConditionItem"/>
						</OverflowToolbar>
					</t:extension>
					<t:columns>
						<t:Column width="11rem" hAlign="Center">
							<Label text="Material Number"/>
							<t:template>
								<Input id="idMaterialNumber" class="mtid" textFormatMode="KeyValue" placeholder=" Enter Material No" showSuggestion="true"
									showValueHelp="true" value="{PurchaseModel>Material}" valueHelpRequest="handlePOMaterialHelp"
									suggestionItems="{ path: 'Lookup>/MaterialList' }" suggestionItemSelected="onSuggest" liveChange="materialchange">
									<suggestionItems>
										<core:ListItem key="{Lookup>Materialno}" text="{Lookup>Description}" additionalText="{Lookup>Materialno}"/>
									</suggestionItems>
								</Input>
								<!--	<Input id="idMaterialNumber" placeholder="Enter Material No" showSuggestion="true" showValueHelp="true"
								valueHelpRequest="handleValueHelpMaterial" 	value="{PurchaseModel>Material}" suggestionItems="{VHeader>/MaterialmasterSet}"
								suggestionItemSelected="MaterialItemSelected">
								<suggestionItems>
									<core:Item key="{VHeader>Materialno}"/>
								</suggestionItems>
							</Input>-->
							</t:template>
						</t:Column>
						<t:Column hAlign="Center" width="11rem">
							<Label text="Material Description"/>
							<t:template>
								<Input class="desc1" id="nDescription" textFormatMode="KeyValue" placeholder="Enter Material Description" showSuggestion="true"
									showValueHelp="true" valueHelpRequest="handleMaterialDisVendor" suggestionItems="{ path: 'Lookup>/MaterialDiscription' }"
									value="{PurchaseModel>ShortText}" suggestionItemSelected="onSuggest">
									<suggestionItems>
										<core:ListItem key="{Lookup>Description}" text="{Lookup>Materialno}" additionalText="{Lookup>Description}"/>
									</suggestionItems>
								</Input>
								<!--	<Input class="desc1" id="nDescription" placeholder="Enter Material Description"  showSuggestion="true" showValueHelp="true"
							 	value="{PurchaseModel>ShortText}" valueHelpRequest="handleValueHelpMaterialDiscription" editable="{EditModel>/isEditable}" suggestionItems="{VHeader>/MaterialmasterSet}"
								suggestionItemSelected="MaterialDiscriptionItemSelected">
								<suggestionItems>
									<core:Item text="{VHeader>Description}"/>
								</suggestionItems>
							</Input>-->
							</t:template>
						</t:Column>
						<t:Column width="9rem" hAlign="Center">
							<Label text="PO Quantity"/>
							<t:template>
								<Input id="Quantity" value="{PurchaseModel>Quantity}" editable="{EditModel>/isEditable}" placeholder="Enter PO Quantity"/>
							</t:template>
						</t:Column>
						<t:Column width="9rem" hAlign="Center">
							<Label text="Unit of Measure"/>
							<t:template>
								<Input id="uom1" class="measure1" value="{PurchaseModel>PoUnit}" editable="{EditModel>/isEditable}"/>
							</t:template>
						</t:Column>
						<t:Column width="9rem" hAlign="Center">
							<Label text="Plant"/>
							<t:template>
								<Input id="Plant" textFormatMode="KeyValue" placeholder=" Enter Plant No" showSuggestion="true" showValueHelp="true"
									value="{PurchaseModel>Plant}" valueHelpRequest="handleValueHelpPlant" suggestionItems="{ path: 'Lookup>/POPlant' }"
									suggestionItemSelected="onSuggest">
									<suggestionItems>
										<core:ListItem key="{Lookup>Bwkey}" text="{Lookup>Bwkey}" additionalText="{Lookup>Bwkey}"/>
									</suggestionItems>
								</Input>
								<!--<Input id="Plant" placeholder="Enter Plant" showSuggestion="true" showValueHelp="true" editable="{EditModel>/isEditable}" value="{PurchaseModel>Plant}"
								valueHelpRequest="handleValueHelpPlant" suggestionItems="{VHeader>/get_plant_f4helpSet}"  suggestionItemSelected="PlantItemSelected">
								<suggestionItems>
									<core:Item text="{VHeader>Bwkey}"/>
								</suggestionItems>
							</Input>-->
							</t:template>
						</t:Column>
						<t:Column width="9rem" hAlign="Center">
							<Label text="Price"/>
							<t:template>
								<Input id="Price" class="price1" value="{PurchaseModel>NetPrice}" editable="{EditModel>/isEditable}"/>
							<!--	<u:Currency textAlign="Center" stringValue="{PurchaseModel>NetPrice}" currency="{ }" useSymbol="true"/>
						-->
							</t:template>
						</t:Column>
					</t:columns>
					<t:footer>
						<Toolbar>
							<ToolbarSpacer/>
							<Button id="idSave"  icon="sap-icon://save" text="Save" press="onSavePurchaseOrder"/>
							<Button icon="sap-icon://decline" text="{i18n>cancel}" press="OnCancel"/>
						</Toolbar>
					</t:footer>
				</t:Table>
			</VBox>
		</content>
	</Page>
</mvc:View>
