	<Page showHeader="true" title="Display Vendor :Initial" enableScrolling="false">
		<headerContent>
			<Button id="btn_save" icon="sap-icon://edit" press="onEditPress" tooltip="save"/>
			<Button id="btn_cancel" icon="sap-icon://decline" press="OnCancelContract" tooltip="decline"/>
		</headerContent>
		<content>
			<VBox class="sapUiSmallMargin">
				<form:SimpleForm id="SimpleFormChange480_12120" editable="true" layout="ResponsiveGridLayout" title="Vendor Display" labelSpanXL="4"
					labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0"
					columnsXL="2" columnsL="2" columnsM="1" singleContainerFullSize="false">
					<form:content>
						<core:Title text="Office"/>
						<Label text="Vendor Number"/>
						<Input id="idVendor" width="80%" textFormatMode="KeyValue" placeholder="Vendor" showSuggestion="true" showValueHelp="true"
							value="{CreateVendor>/VendorContract/Lifnr}" valueHelpRequest="handleMaterialValueHelp"
							suggestionItems="{ path: 'Lookup>/DisplyaVendorList' }" suggestionItemSelected="onSuggest">
							<suggestionItems>
								<core:ListItem key="{Lookup>Lifnr}" text="{Lookup>Name1}" additionalText="{Lookup>Lifnr}"/>
							</suggestionItems>
						</Input>
					<Label text="Vendor Title" required="true"/>
						<Select id="vtitle" selectedKey="vtitle" >
							<items>
								<core:Item text="Mr." key="Mr"/>
								<core:Item text="Ms." key="Ms"/>
								<core:Item text="MADM." key="MADM"/>
								<core:Item text="MISS." key="MISS"/>
								<core:Item text="MIST." key="MIST"/>
								<core:Item text="Company" key="Company"/>
								<core:Item text="DOCT." key="DOCT"/>
								<core:Item text="Dr." key="Dr"/>
							</items>
								<layoutData>
								<l:GridData span="XL2 L3 M4 S4"/>
							</layoutData>
						</Select>
						<Input width="68%" value="{CreateVendor>/VendorContract/FirstName}" id="idFname" editable="{EditModel>/isEditable}" placeholder=" Firstname ..."></Input>
						<Label text="LastName" required="true"/>
						<Input width="80%" value="{CreateVendor>/VendorContract/LastName}" editable="{EditModel>/isEditable}" id="idLname" placeholder="LastName ..."/>
						<Label text="Telphone"/>
						<Input width="80%" value="{CreateVendor>/VendorContract/Telephone}" id="idTel" editable="{EditModel>/isEditable}" class="InputBoxContainer"
							placeholder=" Enter Telephone ..."/>
					
						<core:Title text=" "/>
							<Label text="Account Group" required="true"/>
						<Input id="idAccGrp" width="60%" textFormatMode="KeyValue" placeholder="Account Group ..." editable="{EditModel>/isEditable}"
							value="{CreateVendor>/VendorContract/AccountGrp}" showSuggestion="true" showValueHelp="true" valueHelpRequest="handleAccountCodeVendor"
							suggestionItems="{ path: 'Lookup>/AccountGroup' }" suggestionItemSelected="suggestionItemAccount">
							<suggestionItems>
								<core:ListItem key="{Lookup>Ktokk}" text="{Lookup>Ktokk}" additionalText="{Lookup>Ktokk}"/>
							</suggestionItems>
						</Input>
						<Label text="Purchase Org" required="true"/>
						<Input id="idPurOrg" width="60%" textFormatMode="KeyValue" placeholder=" Purchase Org .." showSuggestion="true" editable="{EditModel>/isEditable}"
							showValueHelp="true" value="{CreateVendor>/VendorContract/PurchaseOrg}" valueHelpRequest="handlePurchaseOrgVendor"
							suggestionItems="{ path: 'Lookup>/PurchaseOrganization' }" suggestionItemSelected="onSuggest">
							<suggestionItems>
								<core:ListItem key="{Lookup>Ekorg}" text="{Lookup>Ekotx}" additionalText="{Lookup>Ekorg}"/>
							</suggestionItems>
						</Input>
						<Label text="Company Code" required="true"/>
						<Input id="idCompCode" width="60%" textFormatMode="KeyValue" editable="{EditModel>/isEditable}" placeholder="Company Code ..." showSuggestion="true"
							value="{CreateVendor>/VendorContract/CompCode}" showValueHelp="true" valueHelpRequest="handleCompanyCodeVendor"
							suggestionItems="{ path: 'Lookup>/CountryCode' }" suggestionItemSelected="suggestionItemCompCode">
							<suggestionItems>
								<core:ListItem key="{Lookup>Bukrs}" text="{Lookup>Butxt}" additionalText="{Lookup>Bukrs}"/>
							</suggestionItems>
						</Input>
							<Label text="OrderCurrency" required="true" class="sapUiTinyMarginTop"/>
						<Input id="idOrderCur" value="{CreateVendor>/VendorContract/OrderCurrency}" editable="{EditModel>/isEditable}" width="60%"
							class="InputBoxContainer" type="Text" placeholder=" Select Order Currency ..."></Input>
					</form:content>
				</form:SimpleForm>
				<form:SimpleForm id="SimpleFormChange480_12120-2" editable="true" layout="ResponsiveGridLayout" title="Address Details" labelSpanXL="4"
					labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0"
					columnsXL="2" columnsL="2" columnsM="1" singleContainerFullSize="false">
					<form:content>
						<core:Title text="Contact data"/>
						<Label text="Country Code" required="true"/>
						<Input width="80%" id="idCountryCode" textFormatMode="KeyValue" placeholder=" Country Code .." showSuggestion="true" editable="{EditModel>/isEditable}"
							value="{CreateVendor>/VendorContract/Country}" showValueHelp="true" valueHelpRequest="handleValueHelpCountryCode"
							suggestionItems="{ path: 'Lookup>/CountryCodeRegion' }" suggestionItemSelected="suggestionItemCountryCode">
							<suggestionItems>
								<core:ListItem key="{Lookup>Land1}" text="{Lookup>Landx}" additionalText="{Lookup>Land1}"/>
							</suggestionItems>
						</Input>
						<Label text="Address Number"/>
						<Input id="idAddNo" width="80%" value="{CreateVendor>/VendorContract/AddressNum}" editable="{EditModel>/isEditable}" type="Text"
							class="InputBoxContainer" placeholder="Address Number"></Input>
						<Label text="Postal Code" required="true"/>
						<Input width="80%" value="{CreateVendor>/VendorContract/PostalCode}" editable="{EditModel>/isEditable}" class="InputBoxContainer" id="idPostcode"
							placeholder=" Postal Code ..."/>
						<core:Title text=" "/>
						<Label text="Region/District" required="true"/>
						<Input id="idRegion" placeholder="Select Region ..." editable="{EditModel>/isEditable}" type="Text"
							value="{CreateVendor>/VendorContract/Region}" showSuggestion="true" showValueHelp="true" valueHelpRequest="handleValueHelpCust"
							suggestionItems="{ path: 'hierarchy>/results'}" suggestionItemSelected="suggestionItemRegion">
							<suggestionItems>
								<core:Item key="{hierarchy>Bezei}" text="{hierarchy>Bland}"/>
							</suggestionItems>
							<layoutData>
								<l:GridData span="XL2 L3 M4 S4"/>
							</layoutData>
						</Input>
						<Input width="37%" value="{CreateVendor>/VendorContract/District}" class="InputBoxContainer" editable="{EditModel>/isEditable}" id="idDis"
							placeholder="Enter District ..."/>
						<Label text="Street"/>
						<Input width="60%" value="{CreateVendor>/VendorContract/Street}" id="idStreet" editable="{EditModel>/isEditable}" placeholder=" Your Street..."/>
						<Label text="City"/>
						<Input width="60%" value="{CreateVendor>/VendorContract/City}" id="idCity" editable="{EditModel>/isEditable}" class="InputBoxContainer"
							placeholder=" City ..."/>
					</form:content>
				</form:SimpleForm>
			</VBox>
		</content>
	</Page>
	<!--</Page>-->
</mvc:View>