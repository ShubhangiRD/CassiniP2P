<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Recon Account List"
		class="sapUiPopupWithPadding"
		items="{Lookup>/}"
		search="_handleReconAccountSearch"
		confirm="_handleReconAccountClose"
		cancel="_handleReconAccountClose">
		<StandardListItem
			title="{Lookup>Bwkey}"
			info="{Lookup>Bwkey}" />
	</SelectDialog>
</core:FragmentDefinition>