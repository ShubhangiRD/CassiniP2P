<core:FragmentDefinition xmlns:core="sap.ui.core"  xmlns:mvc="sap.ui.core.mvc"  xmlns="sap.m"
	height="100%">
	<ScrollContainer width="100%" horizontal="true" vertical="true" focusable="true">
	
	
		
		<List id="VendorList" items="{Lookup>/DisplyaVendorList}" mode="SingleSelectMaster" itemPress="onSelectionChange">
					<items>
						<ObjectListItem type="Navigation" press="onSelectionChange" title="{Lookup>Lifnr}" intro="{Lookup>Name1}"/>
					</items>
					<headerToolbar>
					<Toolbar>
						<Title id="tableHeader" text=""/>
						<ToolbarSpacer />
						<SearchField
							id="searchFieldVendor"
							tooltip="{i18n>worklistSearchTooltip}"
							search="searchFieldVendor"
							width="auto">
						</SearchField>
					</Toolbar>
				</headerToolbar>
				</List>
	</ScrollContainer>
</core:FragmentDefinition>