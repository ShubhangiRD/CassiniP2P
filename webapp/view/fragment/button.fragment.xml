<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:t="sap.ui.table"  xmlns:l="sap.ui.layout"
	xmlns:form="sap.ui.layout.form" xmlns:f="sap.f" displayBlock="true" xmlns:u="sap.ui.unified"
 xmlns:uxap="sap.uxap" xmlns:core="sap.ui.core">
	<uxap:ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="true">
					<uxap:headerTitle>
						<uxap:ObjectPageHeader objectTitle=" {i18n>createAgreement}" showPlaceholder="true">
							<uxap:actions>
								<uxap:ObjectPageHeaderActionButton type="Emphasized"  id="idSave" text="{i18n>save}" press="onSaveAgreement" hideText="false"
									hideIcon="true"/>
								<uxap:ObjectPageHeaderActionButton type="Emphasized" text="Cancel" hideText="false" press="onCancelAgreement" hideIcon="true"/>
								<uxap:ObjectPageHeaderActionButton  icon="sap-icon://nav-back" type="Emphasized" press="onNavBack"/>

							</uxap:actions>
						</uxap:ObjectPageHeader>
						<!--This is header fragment which is used for the customer which is used for the creating contract.
						this is mandatory-->
					</uxap:headerTitle>
				
				</uxap:ObjectPageLayout>
			
	
				 
	
	</core:FragmentDefinition>