<mvc:View xmlns:core="sap.ui.core" xmlns="sap.uxap" xmlns:l="sap.ui.layout" xmlns:m="sap.m" xmlns:form="sap.ui.layout.form" xmlns:f="sap.f"
	xmlns:card="sap.f.cards" xmlns:html="http://www.w3.org/1999/xhtml" class="sapUiSizeCompact" xmlns:mvc="sap.ui.core.mvc" height="100%"
	controllerName="com.cassiniProcureToPay.controller.VendorDetails">
	<core:Fragment fragmentName="com.cassiniProcureToPay.view.fragment.Header" type="XML"/>
	<ObjectPageLayout id="OPL1" enableLazyLoading="true" useIconTabBar="true" showTitleInHeaderContent="true" upperCaseAnchorBar="false">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<expandedHeading>
					<m:Title text="Vendor Details" wrapping="true"/>
				</expandedHeading>
				<snappedHeading>
					<m:FlexBox fitContainer="true" alignItems="Center">
						<m:Title text="Vendor Details" wrapping="true"/>
					</m:FlexBox>
				</snappedHeading>
				<actions>
			<!--		<ObjectPageHeaderActionButton text="Edit" type="Emphasized" hideText="false" tooltip="edit" press="onEditPress"/>
			
	
				-->		<ObjectPageHeaderActionButton hideText="false"  text="Close" tooltip="decline"/><ObjectPageHeaderActionButton type="Emphasized" hideText="false" text="Save" tooltip="save" press="onSaveContract"/>
						<!--		<ObjectPageHeaderActionButton id="btn_display" type="Emphasized" hideText="false" text="Display" press="onDisplayPress" tooltip="display"/>
		-->		</actions>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent>
			<m:FlexBox wrap="Wrap" fitContainer="true">
				<form:SimpleForm id="sForm" class="sapUiSmallMargin" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4"
					labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" columnsXL="4" columnsL="4" columnsM="1" singleContainerFullSize="false">
					<form:content>
						<core:Title text=""/>
						<m:Label text="Vendor" required="true"/>
						<m:Input id="idVendor" value="{VendorModel>/Vendor}" textFormatMode="KeyValue" placeholder="Vendor Number" showSuggestion="true"
							showValueHelp="true" valueHelpRequest="handleVendorValueHelpBox" suggestionItems="{ path: 'Lookup>/DisplyaVendorList' }"
							suggestionItemSelected="onSuggest" >
								<m:suggestionItems>
								<core:ListItem key="{Lookup>Lifnr}" text="{Lookup>Name1}" additionalText="{Lookup>Lifnr}"/>
							</m:suggestionItems>
						</m:Input>
						<core:Title text=""/>
						<m:Label text="Purchase Org" required="true"/>
						<m:Input id="idPurOrg" value="{VendorModel>/PurchasingOrg}" textFormatMode="KeyValue" placeholder=" Purchase Org" showSuggestion="true"
							showValueHelp="true" valueHelpRequest="handlePurchaseOrgVendor"  suggestionItems="{ path: 'Lookup>/PurchaseOrganization' }"
							suggestionItemSelected="onSuggest">
							<m:suggestionItems>		<core:ListItem key="{Lookup>Ekorg}" text="{Lookup>Ekotx}" additionalText="{Lookup>Ekorg}"/>
							</m:suggestionItems>
						</m:Input>
						<core:Title text=""/>
						<m:Label text="Company Code" required="true"/>
							<m:Input id="idCompCode"   value="{VendorModel>/CompCode}" textFormatMode="KeyValue" placeholder="Company Code" showSuggestion="true" showValueHelp="true"
							 	valueHelpRequest="handleCompanyCodeVendor"  suggestionItems="{ path: 'Lookup>/CountryCode' }" suggestionItemSelected="onSuggest">
								<m:suggestionItems>
									<core:ListItem key="{Lookup>Bukrs}" text="{Lookup>Butxt}" additionalText="{Lookup>Bukrs}"/>
								</m:suggestionItems>
							</m:Input>
								<core:Title text=""/>
								<m:Label  text="Account Group" required="true"/>
							<m:Input id="idAccGp" textFormatMode="KeyValue" placeholder="Account Group ..." showSuggestion="true" showValueHelp="true"
									value="{VendorModel>/VendorAccountGroup}"		valueHelpRequest="handleAccountCodeVendor"  suggestionItems="{ path: 'Lookup>/AccountGroup' }" suggestionItemSelected="onSuggest">
										<m:suggestionItems>
								
												<core:ListItem key="{Lookup>Ktokk}" text="{Lookup>Ktokk}" additionalText="{Lookup>Ktokk}"/>
										</m:suggestionItems>
								
										</m:Input>
					</form:content>
				</form:SimpleForm>
			</m:FlexBox>
		</headerContent>
		<sections>
<!--			<ObjectPageSection titleUppercase="false" title="Initial">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<core:Fragment fragmentName="com.cassiniProcureToPay.view.VendorFragment.Initial" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>-->
			<ObjectPageSection titleUppercase="false" title="Address">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<core:Fragment fragmentName="com.cassiniProcureToPay.view.VendorFragment.Address" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Control">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<core:Fragment fragmentName="com.cassiniProcureToPay.view.VendorFragment.Control" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Payment">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<core:Fragment fragmentName="com.cassiniProcureToPay.view.VendorFragment.Payment" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="ContactPerson">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<core:Fragment fragmentName="com.cassiniProcureToPay.view.VendorFragment.ContactPerson" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="AccountingInfo">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<core:Fragment fragmentName="com.cassiniProcureToPay.view.VendorFragment.AccountingInfo" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="PaymentTransaction">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<core:Fragment fragmentName="com.cassiniProcureToPay.view.VendorFragment.PaymentTransaction" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Correspondence">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<core:Fragment fragmentName="com.cassiniProcureToPay.view.VendorFragment.Correspondence" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="PurchaseData">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<core:Fragment fragmentName="com.cassiniProcureToPay.view.VendorFragment.PurchaseData" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="PartnerFunction">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<core:Fragment fragmentName="com.cassiniProcureToPay.view.VendorFragment.PartnerFunction" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</mvc:View>